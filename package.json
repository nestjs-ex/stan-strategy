{
  "name": "@nestjs-ex/stan-strategy",
  "version": "8.0.0",
  "description": "NATS streaming server strategy and client for nestjs microservice based on the stan.js package",
  "author": "pnthanh",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "test": "jest --config=jest.json --detectOpenHandles",
    "test:watch": "jest --config=jest.json --watch",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^9.0.0",
    "@nestjs/microservices": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^9.0.0",
    "nats": "^1.4.12",
    "node-nats-streaming": "^0.3.2"
  },
  "dependencies": {
    "nanoid": "3.3.4",
    "nats": "1.4.12",
    "node-nats-streaming": "0.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-angular": "19.7.1",
    "@nestjs/common": "9.4.3",
    "@nestjs/core": "9.4.3",
    "@nestjs/microservices": "9.4.3",
    "@nestjs/testing": "9.4.3",
    "@types/chai": "4.3.20",
    "@types/chai-as-promised": "8.0.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.5",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "chai": "4.5.0",
    "chai-as-promised": "8.0.1",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-import": "2.31.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "15.4.3",
    "prettier": "3.5.2",
    "reflect-metadata": "0.1.14",
    "release-it": "18.1.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.2",
    "sinon": "19.0.2",
    "ts-jest": "29.2.5",
    "typescript": "5.7.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjs/jwt"
  },
  "publishConfig": {
    "access": "public"
  }
}
